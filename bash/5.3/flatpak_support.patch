+#ifdef FLATPAK_SUPPORT
+/* Check if a command looks like a Flatpak application ID */
+static int
+looks_like_flatpak_app_id (const char *cmd)
+{
+  const char *p;
+  int dot_count = 0;
+  
+  if (cmd == 0 || *cmd == 0)
+    return 0;
+    
+  /* Flatpak app IDs shouldn't contain slashes */
+  if (strchr(cmd, '/') != 0)
+    return 0;
+    
+  /* Check if it starts or ends with a dot */
+  if (*cmd == '.' || cmd[strlen(cmd) - 1] == '.')
+    return 0;
+    
+  /* Check if it contains at least one dot */
+  for (p = cmd; *p; p++)
+    {
+      if (*p == '.')
+        dot_count++;
+    }
+    
+  /* Require at least 1 dot for a valid-looking Flatpak app ID */
+  return (dot_count >= 1);
+}
+
+/* Check if flatpak command is available */
+static int
+flatpak_command_available (void)
+{
+  char *cmd;
+  int available;
+  
+  cmd = search_for_command ("flatpak", 0);
+  available = cmd != 0;
+  free (cmd);
+  
+  return available;
+}
+
+/* Try to execute a command as a Flatpak application */
+static int
+try_execute_flatpak_app (const char *app_id, WORD_LIST *words, int async)
+{
+  WORD_LIST *wl;
+  char *flatpak_cmd;
+  int result;
+  
+  /* Check if flatpak command is available */
+  if (flatpak_command_available () == 0)
+    return -1;
+  
+  /* Build the command: flatpak run app_id [args...] */
+  wl = make_word_list(make_word("flatpak"), 
+                      make_word_list(make_word("run"),
+                      make_word_list(make_word((char *)app_id), words)));
+  
+  /* Execute the flatpak command */
+  flatpak_cmd = savestring("flatpak");
+  result = execute_disk_command(wl, (REDIRECT *)NULL, flatpak_cmd, 
+                                0, 0, async, (struct fd_bitmap *)NULL, 0);
+  free(flatpak_cmd);
+  
+  return result;
+}
+#endif /* FLATPAK_SUPPORT */
